    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <PackageId>MoneyExtensionsEE</PackageId>
    <Version>1.0.0</Version>
    <License>MIT</License>
    <Authors>Éder Ejima</Authors>
    <Company>Éder Ejima</Company>
